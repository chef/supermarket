<div class="login-box">
  <div class="header">
    <h1>Login with</h1>
  </div>

  <div class="contents">
    <ul class="buttons">
      <li class="github">
        <%= link_to 'GitHub', auth_path(:github) %>
      </li>
      <li class="twitter">
        <%= link_to 'Twitter', auth_path(:twitter) %>
      </li>
    </ul>
  </div>

  <p class="help-text">
    <a href="#">What's this?</a>
  </p>
</div>
